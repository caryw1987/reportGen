.register_success{width:550px;height:280px;border:2px solid #E3E2E2;background:#ffffff;margin:0 auto;}
.register_success span{color:#585858;font-size:15px;font-weight:bold;padding:12px 0 5px 28px;display:block;border-bottom:3px solid #058FF3;}
.register_success div{width:400px;margin:0 auto;margin-top:24px;}
.register_success p{color:#F87B00;} 
.add{background:#37ABFF;width:90px;height:18px;color:#ffffff;float:left;margin:40px 20px 0 0;}
