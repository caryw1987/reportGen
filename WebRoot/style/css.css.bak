#main ul{width:158px;margin: 0;padding: 0;}

#main li{list-style: none;text-align: left;}

#main li.menu{padding: 2px 0;width: 100%;}

#main li.button a
{
    display: block;
    font-family: BPreplay,Arial,Helvetica,sans-serif;
    font-size: 15px;
    height: 24px;
    overflow: hidden;
    padding: 6px 20px 0;
    position: relative;
    width: 147px;
}
#main li.button a:hover
{
    text-decoration: none;
}
#main li.button a span
{
    height: 24px;
    position: absolute;
    right: 0;
    top: 0;
    width: 4px;
    display: block;
}
#main li.button a.blue
{
    background: url(../images/ztitle.jpg) no-repeat;
    color: #074384;
	margin-left:10px;
	padding-left:28px;
	font-size:12px;
	color:#666666;
}
*+ html #main li.button a.blue
{
    background: url(../images/ztitle.jpg) no-repeat;
    color: #074384;
	margin-left:-4px;
	padding-left:28px;
	font-size:12px;
	color:#666666;
}
#main li.button a:hover{color:#2076B4}
.dropdown
{
    display: none;
}
.dropdown li
{
    background-color: #373128;
    border: 1px solid #40392C;
