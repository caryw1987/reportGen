.mainright{width:796px;height:760px;float:left;padding:27px 0 0 16px;border-top:1px solid #D9D9D9;border-right:1px solid #D9D9D9;}
.mainright h2{font-size:13px;padding-left:12px;}
.mainright .mainrb{border-top:2px solid #058FF3;width:770px;;margin-top:10px;}
.smallleft{border:1px solid #BCC6D0;width:300px;height:320px;text-align:center;float:left;margin-right:10px;}
.smallleft p{padding:20px 0 5px 0;}
.mainright .smallleft span{background:#EFF7F9;display:block;border-bottom:1px solid #BCC6D0;margin-bottom:15px;padding:8px 0;text-align:center;}
.smallrighttop{border:1px solid #BCC6D0;width:485px;float:left;height:154px;text-align:center;}
.smallrighttop p{padding:20px 0 5px 0;}
.mainright .smallrighttop span{background:#EFF7F9;display:block;border-bottom:1px solid #BCC6D0;margin-bottom:15px;padding:8px 0;text-align:center;}
.mainleft i{position:absolute;z-index:3;left:12px;width:50px;height:30px;background:url(../images/mainmenu_bg.png) no-repeat;-webkit-transition:all .25s ease;-moz-transition:all .25s ease;-o-transition:all .25s ease;-ms-transition:all .25s ease;transition:all .25s ease}
.mainleft i.i1{background-position:10px -12px}
.mainleft i.i2{background-position:10px -46px;}
.mainleft i.i3{background-position:10px -92px}
.mainleft i.i4{background-position:10px -148px}
.mainleft i.i5{background-position:10px -212px}
.mainleft a:hover{background-position:30px -46px;font-weight:bold;}
.mainleft .curr a{font-weight:bold;}
.mainleft .curr{background:#ffffff;border-left:1px solid #D9D9D9;}
.mainleft ul li a{color:#666666;}
