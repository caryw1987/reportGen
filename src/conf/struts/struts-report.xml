 				<param name="root">jsonReportId</param> 
			</result>
			<result type="json" name="CHECK_OK">
 				<param name="root">jsonReportId</param> 
			</result>
			<result type="json" name="CHECK_NOK">
 				<param name="root">jsonReportId</param> 
			</result>
			<result type="json" name="SEND_OK">
 				<param name="root">jsonReportId</param> 
			</result>
			<result type="json" name="FIND_IMAGE_SUCCESS">
 				<param name="root">imagePathMap</param> 
			</result>
			<result type="json" name="SET_DONE">
 				<param name="root">jsonReportId</param> 
			</result>
			<result type="json" name="SET_UN_DONE">
 				<param name="root">jsonReportId</param> 
			</result>
