			<result name="LIST_REPORTS_4RESULTLOGIN">/list_reports_4ResultLogin.jsp</result>
			<result name="LIST_REPORTS_4GENWORD">/list_reports_4GenWord.jsp</result>
			<result name="LIST_REPORTS_4CHECK">/list_reports_4Check.jsp</result>
