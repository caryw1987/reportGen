			<result name="EDIT_REPORT_JSP_4RECORDER">/edit_report_4recorder.jsp</result>
			<result name="EDIT_REPORT_JSP_4ENTRYPERSON">/edit_report_4EntryPerson.jsp</result>
			<result name="ADD_PRICE_JSP">/add_price.jsp</result>
